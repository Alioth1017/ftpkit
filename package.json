{
	"name": "ftpkit",
	"version": "0.0.11",
	"description": "A command line tool for uploading files to FTP/SSH server",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"bin": {
		"ftpkit": "dist/cli.js"
	},
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"test": "vitest",
		"test:coverage": "vitest run --coverage",
		"lint": "biome check src/",
		"format": "biome check --write src/",
		"prepare": "husky",
		"prepublishOnly": "npm run build && np --no-cleanup --yolo --no-publish --any-branch && npm run build"
	},
	"lint-staged": {
		"*.{js,ts,json,md}": [
			"biome check --write --no-errors-on-unmatched"
		]
	},
	"keywords": [
		"note-taking",
		"productivity",
		"ftp",
		"ssh",
		"sftp",
		"upload"
	],
	"author": "Alioth",
	"license": "ISC",
	"files": [
		"dist/**/*",
		"!dist/**/*.map",
		"README.md",
		"LICENSE"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"dependencies": {
		"axios": "^1.7.2",
		"basic-ftp": "^5.0.5",
		"chalk": "^5.3.0",
		"cli-progress": "^3.12.0",
		"commander": "^12.1.0",
		"ssh2-sftp-client": "^12.0.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@types/cli-progress": "^3.11.6",
		"@types/node": "^22.0.2",
		"@types/ssh2-sftp-client": "^9.0.5",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"np": "^10.0.7",
		"tsup": "^8.2.3",
		"typescript": "^5.5.4",
		"vitest": "^4.0.14"
	}
}
